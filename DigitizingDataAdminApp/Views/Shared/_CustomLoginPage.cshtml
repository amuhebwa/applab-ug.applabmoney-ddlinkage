<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - My ASP.NET MVC Application</title>
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/knockout")
    @Scripts.Render("~/bundles/jquery")
    <link href="@Url.Content("~/Content/Gridmvc.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" />
    <script src="@Url.Content("~/Scripts/jquery-1.9.1.min.js")"></script>
    <script src="@Url.Content("~/Scripts/gridmvc.min.js")"></script>


</head>
<body>

    <div class="container wrap sidebar-right">
        @*<div class="container-fluid" style="margin-top: 20px;">
            <div class="row">
                <div class="navbar-header col-mod-12">
                    <h3>@Html.ActionLink("your logo here", "Index", "Login")</h3>
                </div>
            </div>
        </div>*@
        <div class="container" style="margin-top: 100px; margin-bottom: 200px;">
            <div class="vertical-center-row horizontal-center-row">
                <div class="col-sm-12 col-md-10 col-md-offset-1">
                    <div class="center col-md-8" style="float: none; margin: 0 auto;">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>




        <div class="panel-footer col-md-12" style="padding: 20px; margin-top: 10px; min-height">

            <p style="text-align: center;">&copy; @DateTime.Now.Year - LedgerLink Administrator Portal</p>

        </div>

    </div>
    @RenderSection("scripts", required: false)
</body>
</html>
