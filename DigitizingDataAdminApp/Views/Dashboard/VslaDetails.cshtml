@model DigitizingDataAdminApp.Models.VslaInformation

@{
    Layout = "~/Views/Shared/_CustomLayout.cshtml";
    ViewBag.Title = "VslaDetails";
}

<h4 style="text-align:center;" >Details for @Html.DisplayFor(model => model.VslaName) </h4>
<hr />
<table class="table table-bordered">
    <tr>
        <td><strong>@Html.DisplayName("VSLA Code")</strong></td>
        <td>@Html.DisplayFor(model => model.VslaCode)</td>
    </tr>
    <tr>
        <td><strong>@Html.DisplayName("VSLA Name")</strong></td>
        <td>@Html.DisplayFor(model => model.VslaName)</td>
    </tr>
    <tr>
        <td><strong>@Html.DisplayName("Region")</strong></td>
        <td>@Html.DisplayFor(model => model.RegionId)</td>
    </tr>

    <tr>
        <td><strong>@Html.DisplayName("Date Registered")</strong></td>
        <td>@Html.DisplayFor(model => model.DateRegistered)</td>
    </tr>
    <tr>
        <td><strong>@Html.DisplayName("Date linked")</strong></td>
        <td>@Html.DisplayFor(model => model.DateLinked)</td>
    </tr>
    <tr>
        <td><strong>@Html.DisplayName("Physical Address")</strong></td>
        <td>@Html.DisplayFor(model => model.PhysicalAddress)</td>
    </tr>
    <tr>
        <td><strong>@Html.DisplayName("Phone MSISDN")</strong></td>
        <td>@Html.DisplayFor(model => model.VslaPhoneMsisdn)</td>
    </tr>
    <tr>
        <td><strong>@Html.DisplayName("GPS Location")</strong></td>
        <td>@Html.DisplayFor(model => model.GpsLocation)</td>
    </tr>
    <tr>
        <td><strong>@Html.DisplayName("Contact Person")</strong></td>
        <td>@Html.DisplayFor(model => model.ContactPerson)</td>
    </tr>
    <tr>
        <td><strong>@Html.DisplayName("Position of Contact Person in VSLA")</strong></td>
        <td>@Html.DisplayFor(model => model.PositionInVsla)</td>
    </tr>
    <tr>
        <td><strong>@Html.DisplayName("Phone Number")</strong></td>
        <td>@Html.DisplayFor(model => model.PhoneNumber)</td>
    </tr>
    <tr>
        <td><strong>@Html.DisplayName("Responsible CBT")</strong></td>
        <td>@Html.DisplayFor(model => model.CBT)</td>
    </tr>
    <tr>
        <td><strong>@Html.DisplayName("Status of VSLA")</strong></td>
        <td>@Html.DisplayFor(model => model.Status)</td>
    </tr>
</table>
<p>
    <strong>
        @Html.ActionLink("Edit", "EditVsla", new { id = Model.VslaId }) |
                @Html.ActionLink("Back to List", "VslaData")
    </strong>
</p>
